pend(\"keyword\", this.search_keyword); // Send form to backend and get response data\n\n      axios({\n        method: \"post\",\n        url: \"/project/search/\" + localStorage.uid + \"/\",\n        data: formData\n      }).then(function (res) {\n        console.log(res.data);\n\n        if (res.data.status == \"200\") {\n          _this2.proj_data = res.data.project_datail;\n        } else if (res.data.status == \"500\") {\n          console.log(\"Something wrong...\");\n        }\n      });\n    },\n    enterCanvas: function enterCanvas(id) {\n      console.log(\"sending id\");\n    },\n    remove: function remove(id) {\n      console.log(\"sending id\");\n    }\n  }\n};","map":{"version":3,"mappings":";AAkTA,OAAOA,KAAP,MAAkB,wBAAlB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,MAAI,EAAE,aADO;AAEbC,YAAU,EAAE;AACVH,SAAK,EAALA;AADU,GAFC;AAKbI,MALa,kBAKN;AACL,WAAO;AACLC,eAAS,EAAE,cADN;AAELC,oBAAc,EAAE,EAFX;AAGLC,cAAQ,EAAE,CAHL;AAILC,eAAS,EAAE,EAJN;AAKLC,oBAAc,EAAE;AALX,KAAP;AAOD,GAbY;AAcbC,SAda,qBAcH;AACR;AACA,SAAKC,OAAL;AACD,GAjBY;AAkBbC,SAAO,EAAE;AACPD,WADO,qBACG;AAAA;;AACR;AACAV,WAAK,CAAC;AACJY,cAAM,EAAE,KADJ;AAEJC,WAAG,EAAE,cAAcC,YAAY,CAACC,GAA3B,GAAiC;AAFlC,OAAD,CAAL,CAGGC,IAHH,CAGQ,UAACC,GAAD,EAAS;AACfC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACd,IAAhB;;AACA,YAAIc,GAAG,CAACd,IAAJ,CAASiB,MAAT,IAAmB,GAAvB,EAA4B;AAC1B,eAAI,CAACb,SAAL,GAAiBU,GAAG,CAACd,IAAJ,CAASkB,cAA1B;AACF,SAFA,MAEO;AACLC,eAAK,CAAC,wBAAD,CAAL;AACF;AACD,OAVD;AAWD,KAdM;AAgBPC,aAhBO,uBAgBK;AACV,WAAKf,cAAL,GAAsB,IAAtB;AACD,KAlBM;AAmBPgB,cAnBO,wBAmBM;AACX,WAAKhB,cAAL,GAAsB,KAAtB;AACD,KArBM;AAuBPiB,cAvBO,wBAuBM;AACX,WAAKF,SAAL;AACD,KAzBM;AA2BPG,eA3BO,yBA2BO;AACZ,UAAIC,SAAQ,GAAI,IAAIC,QAAJ,EAAhB;AACA,UAAIC,IAAG,GAAIC,QAAQ,CAACC,cAAT,CAAwB,MAAxB,EAAgCC,KAAhC,CAAsC,CAAtC,CAAX;AACAL,eAAS,CAACM,MAAV,CAAiB,MAAjB,EAAyBJ,IAAzB,EAA+BA,IAAI,CAAC5B,IAApC;AAEAD,WAAK,CAAC;AACJY,cAAM,EAAE,MADJ;AAEJC,WAAG,EAAE,kBAFD;AAGJV,YAAI,EAAEwB,SAHF;AAIJO,eAAO,EAAE;AAAE,0BAAgB;AAAlB;AAJL,OAAD,CAAL,CAMGlB,IANH,CAMQ,UAACmB,QAAD,EAAc;AAClBj