user\"\r\n                      placeholder=\"Password\"\r\n                      id=\"password\"\r\n                      name=\"password\"\r\n                      required\r\n                    />\r\n                  </div>\r\n                  <div class=\"col-sm-6\">\r\n                    <input\r\n                      v-model=\"registerForm.repeat_password\"\r\n                      type=\"password\"\r\n                      class=\"form-control form-control-user\"\r\n                      id=\"exampleRepeatPassword\"\r\n                      placeholder=\"Repeat Password\"\r\n                      required\r\n                    />\r\n                  </div>\r\n                </div>\r\n                <button\r\n                  type=\"submit\"\r\n                  class=\"btn btn-primary btn-user btn-block\"\r\n                  @click=\"submitForm()\"\r\n                >\r\n                  Register Account\r\n                </button>\r\n                <hr />\r\n\r\n                <div class=\"text-center\">\r\n                  <a class=\"small\" href=\"/login\"\r\n                    >Already have an account? Login!</a\r\n                  >\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"RegisterView\",\r\n  data() {\r\n    return {\r\n      registerForm: {\r\n        username: \"\",\r\n        password: \"\",\r\n        repeat_password: \"\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    submitForm() {\r\n      if (this.registerForm.password != this.registerForm.repeat_password) {\r\n        alert(\"The entered password is inconsistent!\");\r\n        return;\r\n      }\r\n\r\n      // Declare a form\r\n      let formData = new FormData();\r\n      formData.append(\"username\", this.registerForm.username);\r\n      formData.append(\"password\", this.registerForm.password);\r\n\r\n      // Send form to backend and get response data\r\n      axios({\r\n        method: \"post\",\r\n        url: \"/register/\",\r\n        data: formData,\r\n      }).then((res) => {\r\n        if (res.data.status == \"200\") {\r\n          console.log(\"register ok!\");\r\n          location.replace(\"/login/\");\r\n        } else if (res.data.status == \"500\") {\r\n          alert(\"Not an unique username!\");\r\n        } else {\r\n          console.log(\"register fails...\");\r\n        }\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n<style scoped>\r\n.body {\r\n  position: fixed;\r\n  background-size: cover;\r\n  padding: 0;\r\n  margin: 0;\r\n  height: 100%;\r\n  width: 100%;\r\n}\r\n</style>\r\n<style scoped src=\"../../new_pages/vendor/fontawesome-free/css/all.min.css\"></style>\r\n<style scoped src=\"../../new_pages/css/sb-admin-2.min.css\"></style>\r\n"]},"metadata":{},"sourceType":"module"}