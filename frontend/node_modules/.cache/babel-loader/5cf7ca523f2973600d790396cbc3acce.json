e.log(res.data);\n      });\n    },\n    enterCanvas: function enterCanvas(id) {\n      console.log(\"sending id\");\n    },\n    remove: function remove(id) {\n      console.log(\"sending id\");\n    }\n  }\n};","map":{"version":3,"mappings":";AAoPA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,MAAI,EAAE,aADO;AAEbC,MAFa,kBAEN;AACL,WAAO;AACLC,eAAS,EAAE,cADN;AAELC,iBAAW,EAAE,EAFR;AAGLC,cAAQ,EAAE,CAHL;AAILC,eAAS,EAAE;AAJN,KAAP;AAMD,GATY;AAUbC,SAVa,qBAUH;AACR;AACA,SAAKC,OAAL;AACD,GAbY;AAcbC,SAAO,EAAE;AACPD,WADO,qBACG;AAAA;;AACR;AACAR,WAAK,CAAC;AACJU,cAAM,EAAE,KADJ;AAEJC,WAAG,EAAE,cAAcC,YAAY,CAACC,GAA3B,GAAiC;AAFlC,OAAD,CAAL,CAGGC,IAHH,CAGQ,UAACC,GAAD,EAAS;AACfC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACb,IAAhB;;AACA,YAAIa,GAAG,CAACb,IAAJ,CAASgB,MAAT,IAAmB,GAAvB,EAA4B;AAC1B,eAAI,CAACZ,SAAL,GAAiBS,GAAG,CAACb,IAAJ,CAASiB,cAA1B;AACF,SAFA,MAEO;AACLC,eAAK,CAAC,wBAAD,CAAL;AACF;AACD,OAVD;AAWD,KAdM;AAgBPC,UAhBO,oBAgBE;AACP,UAAIC,SAAQ,GAAI,IAAIC,QAAJ,EAAhB;AACA,UAAIC,IAAG,GAAIC,QAAQ,CAACC,cAAT,CAAwB,MAAxB,EAAgCC,KAAhC,CAAsC,CAAtC,CAAX;AACAL,eAAS,CAACM,MAAV,CAAiB,MAAjB,EAAyBJ,IAAzB,EAA+BA,IAAI,CAACvB,IAApC;AAEAD,WAAK,CAAC;AACJU,cAAM,EAAE,MADJ;AAEJC,WAAG,EAAE,kBAFD;AAGJT,YAAI,EAAEoB,SAHF;AAIJO,eAAO,EAAE;AAAE,0BAAgB;AAAlB;AAJL,OAAD,CAAL,CAMGf,IANH,CAMQ,UAACgB,QAAD,EAAc;AAClBd,eAAO,CAACC,GAAR,CAAY,WAAZ;AACD,OARH,WASS,UAACc,KAAD,EAAW;AAChBf,eAAO,CAACC,GAAR,CAAYc,KAAZ;AACD,OAXH;AAYD,KAjCM;AAmCPC,UAnCO,oBAmCE;AACPhB,aAAO,CAACC,GAAR,CAAY,qBAAZ,EADO,CAEP;;AACA,UAAIgB,QAAO,GAAI,IAAIV,QAAJ,EAAf;AACAU,cAAQ,CAACL,MAAT,CAAgB,WAAhB,EAA6B,KAAKzB,SAAlC;AACA8B,cAAQ,CAACL,MAAT,CAAgB,aAAhB,EAA+B,KAAKxB,WAApC,EALO,CAOP;;AACAJ,WAAK,CAAC;AACJU,cAAM,EAAE,MADJ;AAEJC,WAAG,EAAE,kBAFD;AAGJT,YAAI,EAAE+B;AAHF,OAAD,CAAL,CAIGnB,IAJH,CAIQ,UAACC,GAAD,EAAS;AACf;AACA;AACA;AACA;AACA;AACAC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACb,IAAhB;AACD,OAXD;AAYD,KAvDM;AAyDPgC,eAzDO,uBAyDKC,EAzDL,EAyDS;AACdnB,aAAO,CAACC,GAAR,CAAY,YAAZ;AACD,KA3DM;AA6DPmB,UA7DO,kBA6DAD,EA7DA,EA6DI;AACTnB,aAAO,CAACC,GAAR,CAAY,YAAZ;AACD;AA/DM;AAdI,CAAf","names":["axios","name","data","page_name","search_data","proj_idx","proj_data","mounted","getData","methods","method","url","localStorage","uid","then","res","console","log","status","project_datail","alert","upload","form_data","FormData","file","document","getElementById","files","append","headers","response","error","search","formData","enterCanvas","id","remove"],"sourceRoot":"","sources":["/home/jerry/Comp208-Neural-Lego/frontend/src/views/ProjectView.vue"],"sourcesContent":["<template>\n  <div id=\"page-top\">\n    <!-- Page Wrapper -->\n    <div id=\"wrapper\">\n      <!-- Sidebar -->\n      <ul\n        class=\"navbar-nav bg-gradient-primary sidebar sidebar-dark accordion\"\n        id=\"accordionSidebar\"\n      >\n        <!-- Sidebar - Brand -->\n\n        <a\n          class=\"sidebar-brand d-flex align-items-center justify-content-center\"\n        >\n          <!-- <div class=\"sidebar-brand-icon rotate-n-15\">\n                    <i class=\"fas fa-laugh-wink\"></i>\n                </div> -->\n          <div class=\"sidebar-brand-text mx-3\">NEURAL LEGO</div>\n        </a>\n\n        <!-- Divider -->\n        <hr class=\"sidebar-divider my-0\" />\n\n        <!-- Nav Item - Dashboard -->\n        <li class=\"nav-item\">\n          <a class=\"nav-link\" href=\"/profile\">\n            <i class=\"fas fa-fw fa-user\"></i>\n            <span>User</span></a\n          >\n        </li>\n\n        <!-- Divider -->\n        <hr class=\"sidebar-divider my-0\" />\n\n        <!-- Nav Item - Dashboard -->\n        <li class=\"nav-item active\">\n          <a class=\"nav-link\" href=\"/project\">\n            <i class=\"fas fa-fw fa-folder\"></i>\n            <span>Project</span></a\n          >\n        </li>\n\n        <!-- Divider -->\n        <hr class=\"sidebar-divider my-0\" />\n\n        <!-- Nav Item - Dashboard -->\n        <li class=\"nav-item\">\n          <a class=\"nav-link\" href=\"/template\">\n            <i class=\"fas fa-fw fa-table\"></i>\n            <span>Template</span></a\n          >\n        </li>\n\n        <hr class=\"sidebar-divider my-0\" />\n\n        <!-- Nav Item - Dashboard -->\n        <li class=\"nav-item\">\n          <a class=\"nav-link\" href=\"/canvas/\">\n            <i class=\"fas fa-fw fa-palette\"></i>\n            <span>Canvas</span></a\n          >\n        </li>\n\n        <hr class=\"sidebar-divider my-0\" />\n\n        <!-- Nav Item - Dashboard -->\n        <li class=\"nav-item\">\n          <a class=\"nav-link\" href=\"/\">\n            <i class=\"fas fa-fw fa-arrow-circle-left\"></i>\n            <span>Log out</span></a\n          >\n        </li>\n\n        <!-- Divider -->\n        <hr class=\"sidebar-divider d-none d-md-block\" />\n      </ul>\n      <!-- End of Sidebar -->\n\n      <!-- Content Wrapper -->\n      <div id=\"content-wrapper\" class=\"d-flex flex-column\">\n        <!-- Main Content -->\n        <div id=\"content\">\n          <!-- Topbar -->\n          <nav\n            class=\"\n              navbar navbar-expand navbar-light\n              bg-white\n              topbar\n              mb-4\n              static-top\n              shadow\n            \"\n          >\n            <!-- Sidebar Toggle (Topbar) -->\n            <button\n              id=\"sidebarToggleTop\"\n              class=\"btn btn-link d-md-none rounded-circle mr-3\"\n            >\n              <i class=\"fa fa-bars\"></i>\n            </button>\n\n            <!-- Topbar Search -->\n            <form\n              class=\"\n                d-none d-sm-inline-block\n                form-inline\n                mr-auto\n                ml-md-3\n                my-2 my-md-0\n                mw-100\n                navbar-search\n              \"\n            >\n              <div class=\"input-group\">\n                <input\n                  v-model=\"search_data\"\n                  type=\"text\"\n                  class=\"form-control bg-light border-0 small\"\n                  placeholder=\"Search for...\"\n                  aria-label=\"Search\"\n                  aria-describedby=\"basic-addon2\"\n                />\n                <div class=\"input-group-append\">\n                  <button\n                    class=\"btn btn-primary\"\n                    type=\"button\"\n                    @click=\"search()\"\n                  >\n                    <i class=\"fas fa-search fa-sm\"></i>\n                  </button>\n                </div>\n              </div>\n            </form>\n          </nav>\n          <!-- End of Topbar -->\n\n          <!-- Begin Page Content -->\n          <div class=\"container-fluid\">\n            <!-- Project Card Example -->\n            <div class=\"card shadow mb-4\">\n              <div class=\"card-header py-3\">\n                <h6 class=\"m-0 font-weight-bold text-primary\">Projects</h6>\n              </div>\n              <div class=\"card-body\">\n                <div class=\"table-responsive-lg\">\n                  <table class=\"table table-hover\">\n                    <thead class=\"thead-dark\">\n                      <tr>\n                        <th scope=\"col\">ID</th>\n                        <th scope=\"col\">Name</th>\n                        <th scope=\"col\">Time</th>\n                        <th scope=\"col\">Operation</th>\n                      </tr>\n                    </thead>\n                    <tbody>\n                      <tr v-for=\"proj in proj_data\" :key=\"proj.project_ID\">\n                        <th scope=\"col\">{{ proj.project_ID }}</th>\n                        <td>\n                          <button\n                            type=\"button\"\n                            class=\"btn btn-link\"\n                            @click=\"enterCanvas(proj.project_ID)\"\n                          >\n                            {{ proj.project_name }}\n                          </button>\n                        </td>\n\n                        <td>\n                          <button type=\"button\" class=\"btn btn-link\" disabled>\n                            {{ proj.project_time }}\n                          </button>\n                        </td>\n                        <td>\n                          <button\n                            class=\"btn btn-warning\"\n                            @click=\"enterCanvas(proj.project_ID)\"\n                          >\n                            Edit</button\n                          >&ensp;\n                          <button\n                            class=\"btn btn-danger\"\n                            @click=\"remove(proj.project_ID)\"\n                          >\n                            Delete\n                          </button>\n                        </td>\n                      </tr>\n                    </tbody>\n                  </table>\n                </div>\n\n                <!-- End of Sidebar -->\n              </div>\n            </div>\n          </div>\n        </div>\n        <!-- End of Content Wrapper -->\n      </div>\n      <!-- End of Page Wrapper -->\n\n      <!-- Logout Modal-->\n      <div\n        class=\"modal fade\"\n        id=\"logoutModal\"\n        tabindex=\"-1\"\n        role=\"dialog\"\n        aria-labelledby=\"exampleModalLabel\"\n        aria-hidden=\"true\"\n      >\n        <div class=\"modal-dialog\" role=\"document\">\n          <div class=\"modal-content\">\n            <div class=\"modal-header\">\n              <h5 class=\"modal-title\" id=\"exampleModalLabel\">\n                Ready to Leave?\n              </h5>\n              <button\n                class=\"close\"\n                type=\"button\"\n                data-dismiss=\"modal\"\n                aria-label=\"Close\"\n              >\n                <span aria-hidden=\"true\">×</span>\n              </button>\n            </div>\n            <div class=\"modal-body\">\n              Select \"Logout\" below if you are ready to end your current\n              session.\n            </div>\n            <div class=\"modal-footer\">\n              <button\n                class=\"btn btn-secondary\"\n                type=\"button\"\n                data-dismiss=\"modal\"\n              ></button>\n              <a class=\"btn btn-primary\" href=\"/login\">Logout</a>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"ProjectView\",\n  data() {\n    return {\n      page_name: \"project_page\",\n      search_data: \"\",\n      proj_idx: 0,\n      proj_data: {},\n    };\n  },\n  mounted() {\n    // Fetch tasks on page load\n    this.getData();\n  },\n  methods: {\n    getData() {\n      // 向后端发送一个请求，res是后端发给前端的数据\n      axios({\n        method: \"get\",\n        url: \"/project/\" + localStorage.uid + \"/\",\n      }).then((res) => {\n        console.log(res.data);\n        if (res.data.status == 200) {\n          this.proj_data = res.data.project_datail;\n        } else {\n          alert(\"project loading error!\");\n        }\n      });\n    },\n\n    upload() {\n      var form_data = new FormData();\n      var file = document.getElementById(\"file\").files[0];\n      form_data.append(\"file\", file, file.name);\n\n      axios({\n        method: \"post\",\n        url: \"/project/upload/\",\n        data: form_data,\n        headers: { \"Content-Type\": \"multipart/form-data\" },\n      })\n        .then((response) => {\n          console.log(\"response:\");\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n\n    search() {\n      console.log(\"sending search_data\");\n      // Declare a form\n      let formData = new FormData();\n      formData.append(\"page_name\", this.page_name);\n      formData.append(\"search_data\", this.search_data);\n\n      // Send form to backend and get response data\n      axios({\n        method: \"post\",\n        url: \"/project/search/\",\n        data: formData,\n      }).then((res) => {\n        // if (res.data.isLogin == true) {\n        //   console.log(\"login ok!\");\n        // } else {\n        //   console.log(\"login fail!\");\n        // }\n        console.log(res.data);\n      });\n    },\n\n    enterCanvas(id) {\n      console.log(\"sending id\");\n    },\n\n    remove(id) {\n      console.log(\"sending id\");\n    },\n  },\n};\n</script>\n\n<style scoped src=\"../../new_pages/vendor/fontawesome-free/css/all.min.css\"></style>\n<style scoped src=\"../../new_pages/css/sb-admin-2.min.css\"></style>\n"]},"metadata":{},"sourceType":"module"}