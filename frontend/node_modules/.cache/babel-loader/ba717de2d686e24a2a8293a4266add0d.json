ormData\n      }).then(function (res) {\n        // if (res.data.isLogin == true) {\n        //   console.log(\"login ok!\");\n        // } else {\n        //   console.log(\"login fail!\");\n        // }\n        console.log(res.data);\n      });\n    },\n    enterCanvas: function enterCanvas(id) {\n      console.log(\"sending id\");\n    },\n    remove: function remove(id) {\n      console.log(\"sending id\");\n    }\n  }\n};","map":{"version":3,"mappings":";AAuPA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,MAAI,EAAE,aADO;AAEbC,MAFa,kBAEN;AACL,WAAO;AACLC,eAAS,EAAE,cADN;AAELC,iBAAW,EAAE,EAFR;AAGLC,oBAAc,EAAE,CACd;AAAEC,kBAAU,EAAE,GAAd;AAAmBC,oBAAY,EAAE,MAAjC;AAAyCC,oBAAY,EAAE;AAAvD,OADc,EAEd;AAAEF,kBAAU,EAAE,GAAd;AAAmBC,oBAAY,EAAE,MAAjC;AAAyCC,oBAAY,EAAE;AAAvD,OAFc;AAHX,KAAP;AAQD,GAXY;AAYbC,SAZa,qBAYH;AACR;AACA,SAAKC,OAAL;AACD,GAfY;AAgBbC,SAAO,EAAE;AACPD,WADO,qBACG;AAAA;;AACR;AACAV,WAAK,CAAC;AACJY,cAAM,EAAE,KADJ;AAEJC,WAAG,EAAE,cAAcC,YAAY,CAACC;AAF5B,OAAD,CAAL,CAGGC,IAHH,CAGQ,UAACC,GAAD,EAAS;AACfC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACf,IAAhB;;AAEA,YAAIe,GAAG,CAACf,IAAJ,CAASkB,MAAT,IAAmB,GAAvB,EAA4B;AAC1B;AACA,eAAI,CAACf,cAAL,GAAsBY,GAAG,CAACf,IAAJ,CAASG,cAA/B;AACF,SAHA,MAGO;AACLgB,eAAK,CAAC,OAAD,CAAL;AACF;AACD,OAZD;AAaD,KAhBM;AAkBPC,UAlBO,oBAkBE;AACP,UAAIC,SAAQ,GAAI,IAAIC,QAAJ,EAAhB;AACA,UAAIC,IAAG,GAAIC,QAAQ,CAACC,cAAT,CAAwB,MAAxB,EAAgCC,KAAhC,CAAsC,CAAtC,CAAX;AACAL,eAAS,CAACM,MAAV,CAAiB,MAAjB,EAAyBJ,IAAzB,EAA+BA,IAAI,CAACxB,IAApC;AAEAD,WAAK,CAAC;AACJY,cAAM,EAAE,MADJ;AAEJC,WAAG,EAAE,kBAFD;AAGJX,YAAI,EAAEqB,SAHF;AAIJO,eAAO,EAAE;AAAE,0BAAgB;AAAlB;AAJL,OAAD,CAAL,CAMGd,IANH,CAMQ,UAACe,QAAD,EAAc;AAClBb,eAAO,CAACC,GAAR,CAAY,WAAZ;AACD,OARH,WASS,UAACa,KAAD,EAAW;AAChBd,eAAO,CAACC,GAAR,CAAYa,KAAZ;AACD,OAXH;AAYD,KAnCM;AAqCPC,UArCO,oBAqCE;AACPf,aAAO,CAACC,GAAR,CAAY,qBAAZ,EADO,CAEP;;AACA,UAAIe,QAAO,GAAI,IAAIV,QAAJ,EAAf;AACAU,cAAQ,CAACL,MAAT,CAAgB,WAAhB,EAA6B,KAAK1B,SAAlC;AACA+B,cAAQ,CAACL,MAAT,CAAgB,aAAhB,EAA+B,KAAKzB,WAApC,EALO,CAOP;;AACAJ,WAAK,CAAC;AACJY,cAAM,EAAE,MADJ;AAEJC,WAAG,EAAE,