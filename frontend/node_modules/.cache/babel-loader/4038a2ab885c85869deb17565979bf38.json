{"ast":null,"code":"import \"core-js/modules/es.json.stringify.js\";\nimport \"core-js/modules/es.function.name.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"ProjectView\",\n  data: function data() {\n    return {\n      page_name: \"project_page\",\n      search_data: \"\",\n      project_datail: [{\n        project_ID: \"1\",\n        project_name: \"name\",\n        project_time: \"time\"\n      }, {\n        project_ID: \"2\",\n        project_name: \"name\",\n        project_time: \"time\"\n      }]\n    };\n  },\n  mounted: function mounted() {\n    // Fetch tasks on page load\n    this.getData();\n  },\n  methods: {\n    getData: function getData() {\n      var _this = this;\n\n      // 向后端发送一个请求，res是后端发给前端的数据\n      axios({\n        method: \"get\",\n        url: \"/project/?pk=2\",\n        params: {\n          pk: 2\n        }\n      }).then(function (res) {\n        console.log(JSON.stringify(res));\n\n        if (res.data.status == 200) {\n          // window.location = \"/project/\";\n          _this.project_datail = res.data.project_datail;\n        } else {\n          alert(\"error\");\n        }\n      });\n    },\n    upload: function upload() {\n      var form_data = new FormData();\n      var file = document.getElementById(\"file\").files[0];\n      form_data.append(\"file\", file, file.name);\n      axios({\n        method: \"post\",\n        url: \"/project/upload/\",\n        data: form_data,\n        headers: {\n          \"Content-Type\": \"multipart/form-data\"\n        }\n      }).then(function (response) {\n        console.log(\"response:\");\n      })[\"catch\"](function (error) {\n        console.log(error);\n      });\n    },\n    search: function search() {\n      console.log(\"sending search_data\"); // Declare a form\n\n      var formData = new FormData();\n      formData.append(\"page_name\", this.page_name);\n      formData.append(\"search_data\", this.search_data); // Send form to backend and get response data\n\n      axios({\n        method: \"post\",\n        url: \"/project/search/\",\n        data: formData\n      }).then(function (res) {\n        // if (res.data.isLogin == true) {\n        //   console.log(\"login ok!\");\n        // } else {\n        //   console.log(\"login fail!\");\n        // }\n        console.log(res.data);\n      });\n    },\n    enterCanvas: function enterCanvas(id) {\n      console.log(\"sending id\");\n    },\n    remove: function remove(id) {\n      console.log(\"sending id\");\n    }\n  }\n};","map":{"version":3,"mappings":";;AA+SA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,MAAI,EAAE,aADO;AAEbC,MAFa,kBAEN;AACL,WAAO;AACLC,eAAS,EAAE,cADN;AAELC,iBAAW,EAAE,EAFR;AAGLC,oBAAc,EAAE,CACd;AAAEC,kBAAU,EAAE,GAAd;AAAmBC,oBAAY,EAAE,MAAjC;AAAyCC,oBAAY,EAAE;AAAvD,OADc,EAEd;AAAEF,kBAAU,EAAE,GAAd;AAAmBC,oBAAY,EAAE,MAAjC;AAAyCC,oBAAY,EAAE;AAAvD,OAFc;AAHX,KAAP;AAQD,GAXY;AAYbC,SAZa,qBAYH;AACR;AACA,SAAKC,OAAL;AACD,GAfY;AAgBbC,SAAO,EAAE;AACPD,WADO,qBACG;AAAA;;AACR;AACAV,WAAK,CAAC;AACJY,cAAM,EAAE,KADJ;AAEJC,WAAG,EAAE,gBAFD;AAGJC,cAAM,EAAE;AAAEC,YAAE,EAAE;AAAN;AAHJ,OAAD,CAAL,CAIGC,IAJH,CAIQ,UAACC,GAAD,EAAS;AACfC,eAAO,CAACC,GAAR,CAAYC,IAAI,CAACC,SAAL,CAAeJ,GAAf,CAAZ;;AAEA,YAAIA,GAAG,CAACf,IAAJ,CAASoB,MAAT,IAAmB,GAAvB,EAA4B;AAC1B;AACA,eAAI,CAACjB,cAAL,GAAsBY,GAAG,CAACf,IAAJ,CAASG,cAA/B;AACF,SAHA,MAGO;AACLkB,eAAK,CAAC,OAAD,CAAL;AACF;AACD,OAbD;AAcD,KAjBM;AAmBPC,UAnBO,oBAmBE;AACP,UAAIC,SAAQ,GAAI,IAAIC,QAAJ,EAAhB;AACA,UAAIC,IAAG,GAAIC,QAAQ,CAACC,cAAT,CAAwB,MAAxB,EAAgCC,KAAhC,CAAsC,CAAtC,CAAX;AACAL,eAAS,CAACM,MAAV,CAAiB,MAAjB,EAAyBJ,IAAzB,EAA+BA,IAAI,CAAC1B,IAApC;AAEAD,WAAK,CAAC;AACJY,cAAM,EAAE,MADJ;AAEJC,WAAG,EAAE,kBAFD;AAGJX,YAAI,EAAEuB,SAHF;AAIJO,eAAO,EAAE;AAAE,0BAAgB;AAAlB;AAJL,OAAD,CAAL,CAMGhB,IANH,CAMQ,UAACiB,QAAD,EAAc;AAClBf,eAAO,CAACC,GAAR,CAAY,WAAZ;AACD,OARH,WASS,UAACe,KAAD,EAAW;AAChBhB,eAAO,CAACC,GAAR,CAAYe,KAAZ;AACD,OAXH;AAYD,KApCM;AAsCPC,UAtCO,oBAsCE;AACPjB,aAAO,CAACC,GAAR,CAAY,qBAAZ,EADO,CAEP;;AACA,UAAIiB,QAAO,GAAI,IAAIV,QAAJ,EAAf;AACAU,cAAQ,CAACL,MAAT,CAAgB,W