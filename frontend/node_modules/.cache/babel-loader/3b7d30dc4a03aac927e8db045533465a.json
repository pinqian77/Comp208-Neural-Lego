{"ast":null,"code":"import \"core-js/modules/es.regexp.exec.js\";\nimport \"core-js/modules/es.string.replace.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"RegisterView\",\n  data: function data() {\n    return {\n      registerForm: {\n        username: \"\",\n        password: \"\",\n        repeat_password: \"\"\n      }\n    };\n  },\n  methods: {\n    // 200\n    submitForm: function submitForm() {\n      if (this.registerForm.password != this.registerForm.repeat_password) {\n        alert(\"The entered password is inconsistent!\");\n        return;\n      } // Declare a form\n\n\n      var formData = new FormData();\n      formData.append(\"username\", this.registerForm.username);\n      formData.append(\"password\", this.registerForm.password); // Send form to backend and get response data\n\n      axios({\n        method: \"post\",\n        url: \"/register/\",\n        data: formData\n      }).then(function (res) {\n        if (res.data.status == \"200\") {\n          console.log(\"register ok!\");\n          location.replace(\"/login/\");\n        } else if (res.data.status == \"500\") {\n          alert(\"Not an unique username!\");\n        } else {\n          console.log(\"register fails...\");\n        }\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";;AAyEA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,MAAI,EAAE,cADO;AAGbC,MAHa,kBAGN;AACL,WAAO;AACLC,kBAAY,EAAE;AACZC,gBAAQ,EAAE,EADE;AAEZC,gBAAQ,EAAE,EAFE;AAGZC,uBAAe,EAAE;AAHL;AADT,KAAP;AAOD,GAXY;AAabC,SAAO,EAAE;AACP;AACAC,cAFO,wBAEM;AACX,UAAI,KAAKL,YAAL,CAAkBE,QAAlB,IAA8B,KAAKF,YAAL,CAAkBG,eAApD,EAAqE;AACnEG,aAAK,CAAC,uCAAD,CAAL;AACA;AACF,OAJW,CAMX;;;AACA,UAAIC,QAAO,GAAI,IAAIC,QAAJ,EAAf;AACAD,cAAQ,CAACE,MAAT,CAAgB,UAAhB,EAA4B,KAAKT,YAAL,CAAkBC,QAA9C;AACAM,cAAQ,CAACE,MAAT,CAAgB,UAAhB,EAA4B,KAAKT,YAAL,CAAkBE,QAA9C,EATW,CAWX;;AACAL,WAAK,CAAC;AACJa,cAAM,EAAE,MADJ;AAEJC,WAAG,EAAE,YAFD;AAGJZ,YAAI,EAAEQ;AAHF,OAAD,CAAL,CAIGK,IAJH,CAIQ,UAACC,GAAD,EAAS;AACf,YAAIA,GAAG,CAACd,IAAJ,CAASe,MAA