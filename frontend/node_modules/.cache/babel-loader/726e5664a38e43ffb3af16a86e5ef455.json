.roc = url;\n        } else {\n          alert(\"can not get roc!\");\n        }\n      });\n    },\n    getEpoch: function getEpoch() {\n      var _this6 = this;\n\n      console.log(\"getting epoch\");\n      axios({\n        method: \"post\",\n        url: \"/train/epoch/\" + localStorage.uid + \"/\" + localStorage.pid + \"/\"\n      }).then(function (res) {\n        console.log(res);\n        console.log(res.data);\n        console.log(res.data.epoch);\n\n        if (res.status == \"200\") {\n          _this6.finished_epoch = res.data.epoch;\n          _this6.percentage = _this6.finished_epoch / _this6.config.epoch;\n          _this6.percentage = Number(_this6.percentage * 100).toFixed(2);\n          console.log(\"percentage\" + _this6.percentage);\n          console.log(\"f_epoch\" + _this6.finished_epoch);\n        } else {\n          alert(\"epoch error!\");\n        }\n      });\n    },\n    getTrainStatus: function getTrainStatus() {\n      console.log(\"enter get status\");\n      this.getAcc();\n      this.getRoc();\n      this.getEpoch();\n    },\n    // untested\n    run: function run() {\n      var _this7 = this;\n\n      var form_data = new FormData();\n      form_data.append(\"run\", \"true\");\n      axios({\n        method: \"post\",\n        url: \"/train/run/\" + localStorage.uid + \"/\" + localStorage.pid + \"/\",\n        data: form_data\n      }).then(function (res) {\n        console.log(res.data);\n\n        if (res.data.status == \"200\") {\n          console.log(\"zai run le...\"); // this.getTrainStatus(); \n          // this.timer = setInterval(this.getTrainStatus(), 1000);\n\n          _this7.timer = setInterval(function () {\n            if (_this7.finished_epoch == _this7.config.epoch) {\n              alert(\"Train finished!\");\n              clearInterval(_this7.timer);\n              _this7.timer = null;\n              _this7.finished_epoch = 0;\n            }\n\n            console.log(\"ep \" + _this7.config.epoch);\n           