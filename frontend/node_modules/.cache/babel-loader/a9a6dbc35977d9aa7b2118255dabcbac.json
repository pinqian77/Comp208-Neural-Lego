{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"LoginView\",\n  data: function data() {\n    return {\n      loginForm: {\n        username: \"\",\n        password: \"\",\n        next_url: \"/project\"\n      }\n    };\n  },\n  methods: {\n    submitForm: function submitForm() {\n      // Declare a form\n      var formData = new FormData();\n      formData.append(\"username\", this.loginForm.username);\n      formData.append(\"password\", this.loginForm.password);\n      formData.append(\"next_url\", this.loginForm.next_url); // Send form to backend and get response data\n\n      axios({\n        method: \"post\",\n        url: \"/login/\",\n        data: formData\n      }).then(function (res) {\n        if (res.data.isLogin == true) {\n          console.log(\"login ok!\");\n        } else {\n          console.log(\"login fail!\");\n        }\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AA2HA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,MAAI,EAAE,WADO;AAEbC,MAFa,kBAEN;AACL,WAAO;AACLC,eAAS,EAAE;AACTC,gBAAQ,EAAE,EADD;AAETC,gBAAQ,EAAE,EAFD;AAGTC,gBAAQ,EAAE;AAHD;AADN,KAAP;AAOD,GAVY;AAWbC,SAAO,EAAE;AACPC,cADO,wBACM;AACX;AACA,UAAIC,QAAO,GAAI,IAAIC,QAAJ,EAAf;AACAD,cAAQ,CAACE,MAAT,CAAgB,UAAhB,EAA4B,KAAKR,SAAL,CAAeC,QAA3C;AACAK,cAAQ,CAACE,MAAT,CAAgB,UAAhB,EAA4B,KAAKR,SAAL,CAAeE,QAA3C;AACAI,cAAQ,CAACE,MAAT,CAAgB,UAAhB,EAA4B,KAAKR,SAAL,CAAeG,QAA3C,EALW,CAOX;;AACAN,WAAK,CAAC;AACJY,cAAM,EAAE,MADJ;AAEJC,WAAG,EAAE,SAFD;AAGJX,YAAI,EAAEO;AAHF,OAAD,CAAL,CAIGK,IAJH,CAIQ,UAACC,GAAD,EAAS;AACf,YAAIA,GAAG,CAACb,IAAJ,CAASc,OAAT,IAAoB,IAAxB,EAA8B;AAC5BC,iBAAO,CAACC,GAAR,CAAY,WAAZ;AACF,SAFA,MAEO;AACLD,iBAAO,CAACC,GAAR,CAAY,aAAZ;AACF;AACD,OAVD;AAWD;AApBM;AAXI,CAAf","names":["axios","name","data","loginForm","username","password","next_url","methods","submitForm","formData","FormData","append","method","url","then","res","isLogin","console","log"],"sourceRoot":"","sources":["/home/jerry/Comp208-Neural-Lego/frontend/src/views/LoginView.vue"],"sourcesConte