{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"LoginView\",\n  data: function data() {\n    return {\n      loginForm: {\n        username: \"\",\n        password: \"\",\n        next_url: \"/project\"\n      }\n    };\n  },\n  methods: {\n    submitForm: function submitForm() {\n      // 声明一个form表单\n      var formData = new FormData(); // 将数据添加到form表单内\n\n      formData.append(\"username\", this.loginForm.username);\n      formData.append(\"password\", this.loginForm.password);\n      formData.append(\"next_url\", this.loginForm.next_url);\n      axios({\n        method: \"post\",\n        url: \"/login/\",\n        data: formData\n      }).then(function (res) {\n        if (res.data.isLogin == true) {\n          console.log(\"login ok!\");\n        } else {\n          console.log(\"login fail!\");\n        }\n      }); // axios.post({\n      //   url: \"/login\",\n      //   data: {\n      //     username: this.loginForm.username,\n      //     password: this.loginForm.password,\n      //     next_url: this.loginForm.next_url,\n      //   },\n      //   headers: {\n      //     \"Content-Type\": \"application/x-www-form-urlencoded\",\n      //   },\n      // });\n    }\n  }\n};","map":{"version":3,"mappings":"AA2HA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,MAAI,EAAE,WADO;AAEbC,MAFa,kBAEN;AACL,WAAO;AACLC,eAAS,EAAE;AACTC,gBAAQ,EAAE,EADD;AAETC,gBAAQ,EAAE,EAFD;AAGTC,gBAAQ,EAAE;AAHD;AADN,KAAP;AAOD,GAVY;AAWbC,SAAO,EAAE;AACPC,cADO,wBACM;AACX;AACA,UAAIC,QAAO,GAAI,IAAIC,QAAJ,EAAf,CAFW,CAGX;;AACAD,cAAQ,CAACE,MAAT,CAAgB,UAAhB,EAA4B,KAAKR,SAAL,CAAeC,QAA3C;AACAK,cAAQ,CAACE,MAAT,CAAgB,UAAhB,EAA4B,KAAKR,SAAL,CAAeE,QAA3C;AACAI,cAAQ,CAACE,MAAT,CAAgB,UAAhB,EAA4B,KAAKR,SAAL,CAAeG,QAA3C;AAEAN,WAAK,CAAC;AACJY,cAAM,EAAE,MADJ;AAEJC,WAAG,EAAE,SAFD;AAGJX,YAAI,EAAEO;AAHF,OAAD,CAAL,CAIGK,IAJH,CAIQ,UAACC,GAAD,EAAS;AACf,YAAIA,GAAG,CAACb,IAAJ,CAASc,OAAT,IAAoB,IAAxB,EAA8B;AAC5BC,iBAAO,CAACC,GAAR,CAAY,WAAZ;AACF,SAFA,MAEO;AACLD,iBAAO,CAACC,GAAR,CAAY,aAAZ;AACF;AACD,OAV