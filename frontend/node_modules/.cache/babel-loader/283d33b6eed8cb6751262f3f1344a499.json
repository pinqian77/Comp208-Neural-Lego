{"ast":null,"code":"import \"core-js/modules/es.function.name.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"ProjectView\",\n  data: function data() {\n    return {\n      page_name: \"project_page\",\n      search_data: \"\"\n    };\n  },\n  // 在页面加载之前执行mounted方法\n  mounted: function mounted() {\n    var _this = this;\n\n    // 向后端发送一个请求，res是后端发给前端的数据\n    this.axios({\n      url: \"/projectPage/\",\n      method: \"get\"\n    }).then(function (res) {\n      if (res.data.status == 200) {\n        _this.project_datail = res.data.project_datail;\n      } else {\n        alert(\"error\");\n      }\n    });\n  },\n  methods: {\n    upload: function upload() {\n      var form_data = new FormData();\n      var file = document.getElementById(\"file\").files[0];\n      form_data.append(\"file\", file, file.name);\n      axios({\n        method: \"post\",\n        url: \"/project/upload/\",\n        data: form_data,\n        headers: {\n          \"Content-Type\": \"multipart/form-data\"\n        }\n      }).then(function (response) {\n        console.log(\"response:\");\n      })[\"catch\"](function (error) {\n        console.log(error);\n      });\n    },\n    search: function search(event) {\n      event.preventDefault();\n      axios({\n        method: \"post\",\n        url: \"/project/search/\",\n        data: {\n          page_name: this.page_name,\n          keyword: this.search_data\n        }\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";AA4DA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,MAAI,EAAE,aADO;AAEbC,MAFa,kBAEN;AACL,WAAO;AACLC,eAAS,EAAE,cADN;AAELC,iBAAW,EAAE;AAFR,KAAP;AAID,GAPY;AAQb;AACAC,SATa,qBASH;AAAA;;AACR;AACA,SAAKL,KAAL,CAAW;AACTM,SAAG,EAAE,eADI;AAETC,YAAM,EAAE;AAFC,KAAX,EAGGC,IAHH,CAGQ,UAACC,GAAD,EAAS;AACf,UAAIA,GAAG,CAACP,IAAJ,CAASQ,MAAT,IAAmB,GAAvB,EAA4B;AAC1B,aAAI,CAACC,cAAL,GAAsBF,GAAG,CAACP,IAAJ,CAASS,cAA/B;AACF,OAFA,MAEO;AACLC,aAAK,CAAC,OAAD,CAAL;AACF;AACD,KATD;AAUD,GArBY;AAsBbC,SAAO,EAAE;AACPC,UADO,oBACE;AACP,UAAIC,S