ct() {\n      this.$refs.fileRef.dispatchEvent(new MouseEvent(\"click\"));\n    },\n    uploadProject: function uploadProject() {\n      var form_data = new FormData();\n      var file = document.getElementById(\"file\").files[0];\n      form_data.append(\"file\", file, file.name);\n      axios({\n        method: \"post\",\n        url: \"/project/upload/\",\n        data: form_data,\n        headers: {\n          \"Content-Type\": \"multipart/form-data\"\n        }\n      }).then(function (response) {\n        console.log(\"response:\");\n      })[\"catch\"](function (error) {\n        console.log(error);\n      });\n    },\n    search: function search() {\n      var _this3 = this;\n\n      var formData = new FormData();\n      formData.append(\"page_name\", this.page_name);\n      formData.append(\"keyword\", this.search_keyword);\n      axios({\n        method: \"post\",\n        url: \"/project/search/\" + localStorage.uid + \"/\",\n        data: formData\n      }).then(function (res) {\n        console.log(res.data);\n\n        if (res.data.status == \"200\") {\n          _this3.proj_data = res.data.project_datail;\n        } else if (res.data.status == \"500\") {\n          console.log(\"Something wrong...\");\n        }\n      });\n    },\n    enterCanvas: function enterCanvas(id) {\n      console.log(\"sending id\");\n    },\n    remove: function remove(id) {\n      console.log(\"sending id\");\n    }\n  }\n};","map":{"version":3,"mappings":";;;AAoVA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,MAAI,EAAE,aADO;AAEbC,MAFa,kBAEN;AACL,WAAO;AACLC,cAAQ,EAAE;AACRF,YAAI,EAAE,EADE;AAERG,iBAAS,EAAE;AAFH,OADL;AAKLC,eAAS,EAAE,cALN;AAMLC,oBAAc,EAAE,EANX;AAOLC,eAAS,EAAE,EAPN;AAQLC,oBAAc,EAAE;AARX,KAAP;AAUD,GAbY;AAcbC,SAda,qBAcH;AACR;AACA,SAAKC,OAAL;AACD,GAjBY;AAkBbC,SAAO,EAAE;AACPD,WADO,qBACG;AAAA;;AACR;AACAV,WAAK,CAAC;AACJY,cAAM,EAAE,KADJ;AAEJC,WAAG,EAAE,cAAcC,YAAY,CAACC,GAA3B,GAAiC;AAFlC,OAAD,CAAL,CAGGC,IAHH,CAGQ,UAACC,GAAD,EAAS;AACfC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACf,IAAhB;;A